    "badge.foreground": "#0F1016",
    "button.background": "#1BC5C9",
    "button.foreground": "#0F1016",
    "button.hoverBackground": "#39D8DF",
    "input.background": "#151823",
    "input.foreground": "#E2E6F1",
    "focusBorder": "#1BC5C9",
    "list.hoverBackground": "#272A34",
    "list.activeSelectionBackground": "#1BC5C9",
    "list.activeSelectionForeground": "#0F1016",
    "list.inactiveSelectionBackground": "#272A34",
    "list.highlightForeground": "#1BC5C9"
  },
  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#6E737F",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string", "markup.fenced_code.block"],
      "settings": {
        "foreground": "#FF9A3D"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language", "string.regexp"],
      "settings": {
        "foreground": "#FF8140"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#B537FF"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#1BC5C9"
      }
    },
    {
      "scope": ["entity.name.type", "support.type", "support.class"],
      "settings": {
        "foreground": "#62A9C7"
      }
    },
    {
      "scope": ["variable", "meta.definition.variable"],
      "settings": {
        "foreground": "#E2E6F1"
      }
    },
    {
      "scope": ["entity.name.tag", "support.constant", "constant.character"],
      "settings": {
        "foreground": "#FF6A1F"
      }
    },
    {
      "scope": ["punctuation", "meta.brace", "meta.delimiter"],
      "settings": {
        "foreground": "#A5ABB8"
      }
    },
    {
      "scope": ["markup.heading", "markup.bold"],
      "settings": {
        "foreground": "#39D8DF"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#C865FF"
      }
    }
  ]
}
